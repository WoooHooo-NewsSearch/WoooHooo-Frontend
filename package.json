{
  "name": "searchengine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --fix --ext .",
    "test": "vue-cli-service test:unit --coverage --testResultsProcessor=jest-sonar-reporter --silent --watchAll=false"
  },
  "dependencies": {
    "@cnamts/vue-cli-plugin-proxy": "^2.2.1",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "jest-sonar-reporter": "^2.0.0",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "typescript": "^4.0.3",
    "vue": "^2.6.11",
    "vue-cookies": "^1.7.4",
    "vue-infinite-scroll": "^2.0.2",
    "vue-jest": "^3.0.7",
    "vue-line-clamp": "^1.3.2",
    "vue-router": "^3.4.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.7",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.0.1",
    "node-sass": "^4.9.2",
    "sass-loader": "^10.0.3",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": []
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
